<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>lib\one.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="lib\**" />
    <EmbeddedResource Remove="lib\**" />
    <None Remove="lib\**" />
    <Page Remove="lib\**" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="BakeryLibrary">
      <HintPath>..\BakeryLibrary\bin\Debug\net7.0\BakeryLibrary.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
